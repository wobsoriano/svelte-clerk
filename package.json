{
	"name": "svelte-clerk",
	"version": "0.20.4",
	"license": "MIT",
	"packageManager": "pnpm@10.24.0",
	"homepage": "https://github.com/wobsoriano/svelte-clerk#readme",
	"keywords": [
		"clerk",
		"typescript",
		"svelte",
		"auth",
		"authentication",
		"passwordless",
		"session",
		"jwt"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wobsoriano/svelte-clerk.git"
	},
	"bugs": "https://github.com/wobsoriano/svelte-clerk/issues",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint .",
		"release": "pnpm build && changeset publish",
		"format": "prettier --write .",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client/index.d.ts",
			"svelte": "./dist/client/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"svelte": "./dist/server/index.js"
		},
		"./webhooks": {
			"types": "./dist/webhooks.d.ts",
			"svelte": "./dist/webhooks.js"
		},
		"./env": "./dist/env.d.ts"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^2.20.0",
		"svelte": "^5.29.0"
	},
	"peerDependenciesMeta": {
		"@sveltejs/kit": {
			"optional": true
		}
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"client": [
				"./dist/client/index.d.ts"
			],
			"server": [
				"./dist/server/index.d.ts"
			],
			"env.d.ts": [
				"./dist/env.d.ts"
			]
		}
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@clerk/localizations": "^3.29.1",
		"@clerk/testing": "^1.13.21",
		"@dotenvx/dotenvx": "^1.51.1",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/eslint": "^9.6.1",
		"@types/set-cookie-parser": "^2.4.10",
		"@vueuse/core": "^13.9.0",
		"autoprefixer": "^10.4.22",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^15.15.0",
		"postcss": "^8.5.6",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.2.12",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.18",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"vitepress": "^1.6.4"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@clerk/backend": "^2.30.0",
		"@clerk/shared": "^3.44.0",
		"set-cookie-parser": "^2.7.2"
	}
}
