{
	"name": "svelte-clerk",
	"version": "0.5.1",
	"license": "MIT",
	"homepage": "https://github.com/wobsoriano/svelte-clerk#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wobsoriano/svelte-clerk.git"
	},
	"bugs": "https://github.com/wobsoriano/svelte-clerk/issues",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./components": {
			"types": "./dist/components/index.d.ts",
			"svelte": "./dist/components/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"svelte": "./dist/server/index.js"
		},
		"./env": "./dist/env.d.ts"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"components": [
				"./dist/components/index.d.ts"
			],
			"server": [
				"./dist/server/index.d.ts"
			],
			"env.d.ts": [
				"./dist/env.d.ts"
			]
		}
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.0",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.7.2",
		"@sveltejs/package": "^2.3.6",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/set-cookie-parser": "^2.4.10",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.47",
		"publint": "^0.2.11",
		"svelte": "^5.1.0",
		"svelte-check": "^4.0.5",
		"tailwindcss": "^3.4.14",
		"tslib": "^2.8.0",
		"vite": "^5.4.10"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@clerk/backend": "^1.21.1",
		"@clerk/shared": "^2.20.1",
		"@clerk/types": "^4.39.3",
		"set-cookie-parser": "^2.7.1"
	}
}
